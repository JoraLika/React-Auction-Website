{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "warn",
            "tab",
            {"SwitchCase": 1}
        ],
        "no-unused-vars": "warn",
        "quotes": [
            "warn",
            "double"
        ],
        "semi": ["warn", "always", {"omitLastInOneLineBlock": true}]
    },
    "globals": {}
}
